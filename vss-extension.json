{
    "manifestVersion": 1,
    "id": "project-management-widgets",
    "version": "0.0.1",
    "name": "Project Management Widgets",
    "description": "This extension contains project management widgets for Azure DevOps.",
    "publisher": "SpryfoxGmbH",
    "categories": ["Azure Boards"],
    "tags": [
        "project management",
        "dashboards",
        "charts",
        "table"
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/spryfox-analytics/azure-devops-project-management-widgets"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "img/logo.png"
    },
    "demands": ["contribution/ms.vss-dashboards-web.widget-sdk-version-2"],
    "contributions": [
        {
            "id": "capacity_and_effort",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "Capacity and Effort",
                "isNameConfigurable": true,
                "description": "Displays capacity and effort for team members.",
                "catalogIconUrl": "img/logo.png",
                "uri": "capacity_and_effort.html",
                "supportedSizes": [
                    {
                        "rowSpan": 2,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 5
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 5
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        }
    ],
    "files": [
        {
            "path": "capacity_and_effort.html",
            "addressable": true
        },
        {
            "path": "sdk/scripts/VSS.SDK.js",
            "addressable": true
        },
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "README.md",
            "addressable": true
        }
    ],
    "scopes": [
        "vso.project",
        "vso.work_full",
        "vso.identity"
    ],
    "galleryFlags": [
        "Preview",
        "Public"
    ]
}
